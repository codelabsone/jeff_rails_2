<h1> Trade In Your Car </h1>
<%= form_for(@vehicle) do |f| %>
  <p>
    <%= f.label :name %><br/>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :make %><br/>
    <%= f.text_field :make %>
  </p>
  <p>
    <%= f.label :cartype %><br/>
    <%= f.text_field :cartype %>
  </p>
  <p>
    <%= f.label :year %><br/>
    <%= f.number_field :year %>
  </p>
  <p>
    <%= f.label :price %><br/>
    <%= f.number_field :price %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
